.my-head {
  height: 340rpx;
}
.items-center {
  display: flex;
  align-items: center;
}
.has-info {
  flex: 1;
  padding: 0 24rpx;
}
.user-info {
  margin-left: 30rpx;
  color: white;
  font-size: 30rpx;
  flex: 1
}
.avatar {
  width: 162rpx;
  height: 162rpx;
  border-radius: 50%;
}
.name {
  margin-bottom: 40rpx;
}
button.sign-in{
  width: 140rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: white;
  border-radius: 30rpx;
  color: #333333;
  font-size: 26rpx;
  padding: 0;
  font-weight: 400;
  float: right;
}
.not-info {
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  flex: 1;
}
.not-info > button {
  width: 240rpx;
  height: 66rpx;
  line-height: 60rpx;
  border: 3rpx solid white;
  border-radius: 44rpx;
  color: white;
  font-size: 26rpx;
  padding: 0;
  font-weight: 400;
  margin-top: 20rpx;
}
